class Solution:
    def compressedString(self, word: str) -> str:
        comp = ""
        n = len(word)
        count = 0   
        for i in range(n-1):
            if word[i] == word[i+1]:
                count+=1
                if count == 9:
                    comp+= "9"+word[i]
                    count = 0
                else